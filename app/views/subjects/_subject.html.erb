<table class="table table-hover table-responsive-sm">
    <thead class="thead-light">
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Alias</th>
        <th scope="col">Branch</th>
        <th scope="col">Semester</th>
        <th colspan="3">Action</th>
      </tr>
    </thead>
    <tbody>
      <% @subjects.each do |subject| %>
        <tr>
          <td><%= subject.name %></td>
          <td><%= subject.alias %></td>
          <td><%= subject.branch.alias %></td>
          <td><%= subject.semester %></td>
          <td><%= link_to '', edit_subject_path(subject), class:"fa fa-pencil-square-o fa-lg" %></td>
          <td><%= link_to '', subject, method: :delete, data: { confirm: 'Are you sure?' }, class:"fa fa-trash fa-lg", class:"fa fa-trash fa-lg"%></td>
        </tr>
      <% end %>
    </tbody>
  </table>